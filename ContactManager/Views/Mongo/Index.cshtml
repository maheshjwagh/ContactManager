@model IEnumerable<ContactManager.Models.Note>

@{
    ViewBag.Title = "Index";
}

<h2>My Notes</h2>

<table border="1">
    <tr>
        <th>Date</th>
        <th>Note Text</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Text)
            </td>
        </tr>
    }

</table>
<div>  @Html.Partial("Create", new ContactManager.Models.Note())</div>